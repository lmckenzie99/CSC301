-- create structure for the type 'Circle' that holds coords of the center and its radius

load system io.
load system math.

structure circle with
  data center.
  data radius.
  function area with none do
    return 2.0*math@pi*this@radius.
  end
end

let o1 = circle((2.1, 3.2), 5.5).

io @println(o1).

io @println(o1@area()).
